{
  "name": "husky-lintstaged-eslint-prettier-example",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "eslint": "^8.58.0",
    "prettier": "^3.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{html,css,json,md}": [
      "prettier --write"
    ]
  }
}
